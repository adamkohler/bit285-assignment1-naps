@model bit285_assignment1_naps.Models.User
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
	Layout = "_Layout";
}


<h1>New Account Created</h1>
<fieldset>
	<form asp-controller="Naps" asp-action="Confirm_account" method="post">
		<div>
			<label asp-for="FirstName">First Name</label>
			<span>@Model.FirstName</span>
		</div>
		<div>
			<label asp-for="LastName">Last Name</label>
			<span>@Model.LastName</span>
		</div>            <div>
			<label asp-for="Program">Program</label>
			<span>@Model.Program</span>
		</div>
		<div>
			<label asp-for="UserName">Username</label>
			<span>@Model.UserName</span>
		</div>
		<div>
			<label asp-for="Password">Password</label>
			<span>@Model.Password</span>
		</div>
		<div>
			<input type="submit" value="Go to Login" />
			<input type="button" value="Reset" />
		</div>
	</form>
</fieldset>
